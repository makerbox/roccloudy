<span id="notice"><%= notice %></span>
<h1>Order started: <%= @order.created_at %></h1>
<% @order.quantities.each do |po| %>
<div class="po">
	<%= link_to po.product.code, product_path(Product.find_by(code: po.product.code)) %>
	<%= current_user.account.seller_level %>
	<% case current_user.account.seller_level.to_i %>
		  	<% when 1 %>
		  		$<%= po.product.price1 %>
		    <% when 2 %>
		  		$<%= po.product.price2 %>
		    <% when 3 %>
		  		$<%= po.product.price3 %>
		    <% when 4 %>
		  		$<%= po.product.price4 %>
		    <% when 5 %>
		  		$<%= po.product.price5 %>
		    <% when 6 %>
		  		$<%= po.product.rrp %>
	<% end %>
	x <%= po.qty %>

<% end %>
<hr>
